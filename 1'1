//Задано одновимірний масив А розміру 2N.
//Побудувати масив В розміру 2N, розмістивши у першій половині масиву В другу
//половину масиву А, а у другій половині масиву В, першу - А.
#include <stdio.h>
#include <iostream>
using namespace std;
int main()
{
    int N;
    cout<<"Введіть значення N:";
    cin>>N;
   
    int A[2*N], B[2*N];
    //масив А
    cout<<"Введіть "<<2*N<<" елементів масиву А:"<<endl;
    for(int i=0; i<2*N; i++){
        cin>>A[i];
    }
    //масив В
    for(int i=0; i<N; i++){
        B[i]=A[i+N];
        B[i+N]=A[i];
    }
   
    cout<<"Масив В: ";
    for(int i=0; i<2*N; i++){
        cout<<B[i]<<" ";
    }
    cout<<endl;

    return 0;
}
